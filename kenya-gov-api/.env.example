# Environment Variables for Kenya Government APIs Application

# Base URLs for the APIs
ETIMS_BASE_URL=https://developer.go.ke/apis/eTims
ECITIZEN_BASE_URL=https://api.ecitizen.go.ke
GAVACONNECT_BASE_URL=https://api.gavaconnect.go.ke

# API Keys and Credentials
ETIMS_API_KEY=your_etims_api_key
ECITIZEN_USERNAME=your_ecitizen_username
ECITIZEN_PASSWORD=your_ecitizen_password
GAVACONNECT_API_KEY=your_gavaconnect_api_key

# Timeout settings (in milliseconds)
API_TIMEOUT=30000